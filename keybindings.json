[
	{
		"key": "alt+p",
		"command": "workbench.action.quickOpen"
	},
	{
		"key": "ctrl+p",
		"command": "workbench.action.terminal.toggleTerminal"
	},
	{
		"key": "ctrl+p",
		"command": "workbench.action.terminal.focus",
		"when": "!terminalFocus"
	},
	{
		"key": "ctrl+f",
		"command": "-workbench.action.terminal.focusFindWidget",
		"when": "terminalFocus"
	}
]
